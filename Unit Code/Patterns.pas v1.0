unit Patterns(-9234);interface {$U-} {Use My own include Librarys} {$O Jim Surine:Programming:Units:My Units:Patterns}uses Memtypes,            {Basic types stanadard} QuickDraw,           {Quickdraw standard} OSIntf,              {Operating system standard} ToolIntf;            {Tools standard} const    GreyScale=0;        HorzLines=13;    VertLines=16;        LeftLines=11;    RightLines=12;     var    ThePatternHandle:PatHandle;    i:integer;    MyPatterns:array [0..34] of Pattern;implementation begin     For i:= 0 to 16 do      begin        ThepatternHandle:=GetPattern(i+1000);        HLock(Handle(ThepatternHandle));        MyPatterns[i]:=ThepatternHandle^^;        HunLock(Handle(ThepatternHandle));      end;           For i:= 0 to 1 do      begin        ThepatternHandle:=GetPattern(i+1065);        HLock(Handle(ThepatternHandle));        MyPatterns[i+17]:=ThepatternHandle^^;        HunLock(Handle(ThepatternHandle));      end;           For i:= 0 to 2 do      begin        ThepatternHandle:=GetPattern(i+2000);        HLock(Handle(ThepatternHandle));        MyPatterns[i+19]:=ThepatternHandle^^;        HunLock(Handle(ThepatternHandle));      end;           For i:= 0 to 2 do      begin        ThepatternHandle:=GetPattern(i+3000);        HLock(Handle(ThepatternHandle));        MyPatterns[i+22]:=ThepatternHandle^^;        HunLock(Handle(ThepatternHandle));      end;         For i:= 0 to 1 do      begin        ThepatternHandle:=GetPattern(i+4000);        HLock(Handle(ThepatternHandle));        MyPatterns[i+25]:=ThepatternHandle^^;        HunLock(Handle(ThepatternHandle));      end;     For i:= 0 to 1 do      begin        ThepatternHandle:=GetPattern(i+5000);        HLock(Handle(ThepatternHandle));        MyPatterns[i+27]:=ThepatternHandle^^;        HunLock(Handle(ThepatternHandle));      end;     For i:= 0 to 2 do      begin        ThepatternHandle:=GetPattern(i+5500);        HLock(Handle(ThepatternHandle));        MyPatterns[i+29]:=ThepatternHandle^^;        HunLock(Handle(ThepatternHandle));      end;         For i:= 0 to 2 do      begin        ThepatternHandle:=GetPattern(i+5600);        HLock(Handle(ThepatternHandle));        MyPatterns[i+32]:=ThepatternHandle^^;        HunLock(Handle(ThepatternHandle));      end; end.