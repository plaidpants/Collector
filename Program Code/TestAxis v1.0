Program TestAxis(input,Output);  {$U Jim Surine:Programming:Units:My Units:Dialogs} {$U Jim Surine:Programming:Units:My Units:Patterns} {$U Jim Surine:Programming:Units:My Units:Axis2} {$R Jim Surine:Programming:Reasources:Collector.Rsrc} {$I-} {$S+} uses  Memtypes,            {Basic types stanadard} QuickDraw,           {Quickdraw standard} OSIntf,              {Operating system standard} ToolIntf,            {Tools standard}{$S two} PackIntf,            {Package stuff (List Manager, Decimal to string convert)} sane,                {For Real number conversions} Patterns, Dialogs, Axis2; var    test:integer;    AxisInfo : AxisInfoType;    i:integer;    Quit:boolean;    Port:windowPtr;    Reply:SFReply;     TheRealPoint:RealPoint;    ThePoint:Point;    InView:boolean;    begin     quit:=false;    GetPort(Port);    with AxisInfo do        begin            DefaultAxis(AxisInfo,Port);                        DataRect.Bottom:=-50;            DataRect.top:=50;            DataRect.Left:=-50;            DataRect.Right:=50;                        XLog:=0;            YLog:=0;                        XMinorTicks:=5;            YMinorTicks:=5;                        XMajorTicks:=10;            YMajorTicks:=10;                        XGridMinor:=8;            XGridMajor:=16;                        YGridMinor:=8;            YGridMajor:=16;                        Title:='Test the Title of Axis Program';                        Note:='Test Notes';                  end;        while not Quit do        begin            setPort(port);            write('enter >');            readln(test);            case test of                0:DefaultAxis(Axisinfo,Port);                1:CheckAxisinfo(Axisinfo);                2:AdjustAxis(Axisinfo,Port);                3:DrawAxis(Axisinfo,Port);                4:                    begin                        GotoXY(1,1);                        EraseRect(Port^.PortRect);                    end;                5:for i:= 1 to 100 do                    begin                        penPat(MyPatterns[16]);                        TheRealPoint.X:=i*1.0;                        TheRealPoint.y:=i*1.0;                        WhereAxis(AxisInfo,TheRealPoint,ThePoint,InView);                        moveTo(ThePoint.h,ThePoint.v+1);                        line(0,-2);                        move(1,1);                        line(-2,0);                    end;                6:insetRect(AxisInfo.DisplayRect,40,40);                7:AxisInfoDialog (Axisinfo,port);                 8:SaveAxisAsText (Axisinfo,Reply,OutPut);                otherwise                   Quit:=True;            end;        end;end.