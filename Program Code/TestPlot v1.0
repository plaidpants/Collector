Program TestPlot(input,Output);  {$U Jim Surine:Programming:Units:My Units:Dialogs} {$U Jim Surine:Programming:Units:My Units:Patterns} {$U Jim Surine:Programming:Units:My Units:Axis2} {$U Jim Surine:Programming:Units:My Units:RecordInteger} {$U Jim Surine:Programming:Units:My Units:RecordLongInt} {$U Jim Surine:Programming:Units:My Units:RecordReal} {$U Jim Surine:Programming:Units:My Units:Plot} {$R Jim Surine:Programming:Reasources:Collector.Rsrc} {$I-} {$S+} uses  Memtypes,            {Basic types stanadard} QuickDraw,           {Quickdraw standard} OSIntf,              {Operating system standard} ToolIntf,            {Tools standard} PackIntf,            {Package stuff (List Manager, Decimal to string convert)} sane,                {For Real number conversions} {$S two}   Dialogs, Patterns, Axis2,   {$S three} RecordInteger, RecordLongInt, RecordReal, Plot;var    test:integer;    Plot : PlotType;    Numbers : PlotDataType;    i,j:integer;    Quit:boolean;    Port:windowPtr;    Reply:SFReply;    Number:LongInt;    TheFile:Text;    error:Boolean;    begin     quit:=false;    GetPort(Port);    with Plot.PlotInfo do        begin                with AxisInfo do                     begin                         DefaultAxis(AxisInfo,Port);                                                  DataRect.Bottom:=0;                         DataRect.top:=100;                         DataRect.Left:=0;                         DataRect.Right:=100;                                                  XLog:=0;                         YLog:=0;                                                  XMinorTicks:=5;                         YMinorTicks:=5;                                                  XMajorTicks:=10;                         YMajorTicks:=10;                                                  XGridMinor:=0;                         XGridMajor:=8;                                                  YGridMinor:=0;                         YGridMajor:=8;                                                  Title:='Test the Title of Axis Program';                                                  Note:='Test Notes';                     end;                                        TypeOfData:=2;                                        DisplayX:=1;                    DisplayY:=2;                               DisplayPointType:=1;                    DisplayLineType:=0;                               DisplayXError:=0;                    DisplayYError:=4;                               DisplayErrorType:=1;                               PlotDensity:=1;                               PlotStart:=0;                    NumberToPlot:=-1;                               PlotTrack:=1;                    PlotTrackAxis:=1;                        DrawOption:=0;        end;    with plot.PlotData.RecordReal.RecordRealInfo do        begin            NumberPerRecord := 4;            NumberToRecord:=100;            Title :='test record Real';            DrawOption :=0;        end;            InitPlot(Plot,port);        while not Quit do        begin            write('enter >');            readln(test);            case test of                0:DrawPlot(Plot,Port);                1:PlotDisplayDialog(Plot,Port);                2:PlotDataDialog(Plot,Port);                3:begin                    for i:=1 to Plot.PlotData.RecordReal.RecordRealInfo.NumberPerRecord do                        readln(Numbers.RecordRealData[i]);                    AddToPlot(Plot,Numbers,port,error);                  end;                4:                  begin                    Rewrite(TheFile,'Test');                    SavePlotAsText(Plot,Reply,TheFile);                    Close(TheFile);                  end;                5:DrawAxis(Plot.PlotInfo.AxisInfo,Port);                6:                    begin                        GotoXY(1,1);                        EraseRect(Port^.PortRect);                    end;                7:begin                     Readln(Number);                    for i:=1 to Number do                        begin                        for j:=1 to 2 do                            Numbers.RecordRealData[j]:= abs(Random/MaxInt*50)+abs(Random/MaxInt*50);                        for j:=3 to 4 do                            Numbers.RecordRealData[j]:= abs(Random/MaxInt*5)+abs(Random/MaxInt*5);                        AddToPlot(Plot,Numbers,port,error);                        end;                  end;                                  8:Quit:=True;                otherwise                     OKDialog('hello');            end;        end;    ClosePlot(Plot);end.